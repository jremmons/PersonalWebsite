}

@media only screen and (max-width: 941px) {

#navigationGateway {
	position: relative;
	background-color: #c8bfb8;
	border: 1px solid #d9d9d9;
	color: #003876;
	cursor: pointer;
	display: block;
	float: left;
	font-size: 12px;
	font-weight: bold;
	height: 28px;
	line-height: 28px;
	margin: 0 0 0 20px;
	outline: none;
	padding: 0;
	text-align: left;
	text-indent: 20px;
	text-decoration: none;
	width: 150px;
}
#navigationGateway ul {
	background-color: rgba(255, 255, 255, 0.952941);
    height: 210px;
	width: 151px;
    overflow: hidden;
	z-index: 40;
	position: absolute;
	left: -1px;
	top: 29px;
}
#navigationGateway li {
	color: rgb(0, 56, 118);
	cursor: pointer;
	display: list-item;
	font-size: 14px;
	font-weight: bold;
	line-height: 18px;
	margin: 0;
	min-height: 18px;
	padding: 0;
	text-align: center;
	white-space: nowrap;
	width: 151px;
}
#navigationGateway li a {
	color: rgb(85, 85, 85);
	cursor: pointer;
	display: block;
	font-size: 12px;
	font-weight: normal;
	height: 18px;
	line-height: 18px;
	list-style: none;
	padding: 6px 14px;
	text-align: left;
	text-decoration: none;
	text-indent: 0;
	white-space: nowrap;
	width: 123px;
	border: none;
}
#navigationGateway li a:hover {
	color: rgb(85, 85, 85);
	background-color: #89aae3;
	padding: 5px 14px;
	width: 121px;
	border: 1px solid white;
}
#navigationTactical {
	color: #222222;
	display: block;
	float: left;
	height: 32px;
	margin: 0 0 2px 0;
	padding: 0;
	width: 300px;
}
#visit_drake, #contact_drake {
	border: 1px solid #d9d9d9;
	border-left: none;
	background-color: #c8bfb8; 
	color: #003876;
	cursor: pointer;
	display: block;
	float: right;
	font-size: 12px;
	font-weight: bold;
	height: 28px;
	line-height: 28px;
	margin: 0;
	outline: none;
	padding: 0;
	text-align: center;
	width: 149px;
}
#visit_drake a, #contact_drake a, #navigationGateway a {
	display: block;
	text-decoration: none;
	height: 28px;
	width: 149px;
}
#visit_drake:hover, #contact_drake:hover, #navigationGateway:hover {
	color: #ffffff;
	background-color: #102343;
}
#visit_drake a:hover, #contact_drake a:hover, #navigationGateway a:hover {
	color: #ffffff;
}
	
} /* END 941 */

